  "homepage": "https://github.com/gwtw/ts-avl-tree",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^5.1.1",
    "nyc": "^11.4.1",
    "tslint": "^5.9.1",
    "tslint-consistent-codestyle": "^1.13.0",
    "typescript": "^3.0.3"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.js.map",
    "!lib/test",
